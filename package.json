{
  "name": "@jwstott/odata-v4-pg",
  "version": "1.2.1-1",
  "description": "OData to PostgreSQL query compiler",
  "main": "build/lib/index.js",
  "typings": "build/lib/index",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prebuild": "rimraf build",
    "build": "tsc --skipLibCheck",
    "pretest": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jstott/odata-v4-pg.git"
  },
  "keywords": [
    "OData",
    "V4",
    "sql"
  ],
  "author": "JStott",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jstott/odata-v4-pg/issues"
  },
  "homepage": "https://github.com/jstott/odata-v4-pg#readme",
  "dependencies": {
    "@jwstott/odata-v4-parser": "^0.3.0",
    "odata-v4-literal": "^0.1.1",
    "odata-v4-sql": "^0.1.1"
  },
  "devDependencies": {
    "@types/config": "^0.0.39",
    "@types/jest": "^27.0.1",
    "@types/pg": "^8.6.1",
    "config": "^3.3.6",
    "jest": "^27.1.1",
    "knex": "~1.0.4",
    "objection": "3.0.1",
    "odata-v4-server": "rc",
    "pg": "^8.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
